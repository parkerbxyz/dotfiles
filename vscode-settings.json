{
  "telemetry.telemetryLevel": "off",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.startupEditor": "newUntitledFile",
  "editor.codeActionsOnSave": { "source.fixAll.markdownlint": true},
  "editor.fontSize": 14,
  "editor.fontFamily": "'FiraCode Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.scrollBeyondLastLine": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "markdownlint.config": {
    "MD001": true,
    "MD024": false,
    "MD025": { "front_matter_title": "" },
    "MD026": { "punctuation": ".,;:!" },
    "MD033": { "allowed_elements": ["kbd", "details", "summary", "strong", "sup", "sub", "iframe", "br", "img", "pre", "a"] },
    "MD036": true,
    "MD041": true
  },
  "markdown.extension.toc.githubCompatibility": true,
  "markdown.extension.toc.updateOnSave": false,
  "markdown.extension.toc.levels": "2..3",
  "markdown.extension.orderedList.marker": "one",
  "markdown.experimental.validate.enabled": true,
  "markdown-table-formatter.enableSort": false,
  "python.linting.pylintEnabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "diffEditor.renderSideBySide": false,
  "shellcheck.customArgs": [
    "-x"
  ],
  "shellcheck.executablePath": "shellcheck",
  "shellcheck.enableQuickFix": true,
  "git.enableCommitSigning": true,
  "[shellscript]": {
    "editor.rulers": [100]
  },
  "explorer.confirmDelete": false,
  "editor.minimap.renderCharacters": false,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.defaultProfile.linux": "fish",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false,
    "javascript": true
  }
  "redhat.telemetry.enabled": false,
  "git.enableSmartCommit": true,
  "editor.experimental.stickyScroll.enabled": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
}
